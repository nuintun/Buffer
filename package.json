{
  "name": "@nuintun/buffer",
  "version": "0.0.1",
  "description": "A buffer tool for javascript.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "node tests/index.js",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "build:tests": "rollup -c tools/rollup.tests.js",
    "build:examples": "rollup -c tools/rollup.examples.js",
    "format": "prettier --write tools src tests examples cjs esm typings",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:typings",
    "build:typings": "tsc --declaration --emitDeclarationOnly --declarationDir typings",
    "build": "npm run clean && npm run build:main && npm run build:tests && npm run build:examples && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/Buffer.git"
  },
  "keywords": [
    "buffer"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/Buffer/issues"
  },
  "homepage": "https://github.com/nuintun/Buffer#readme",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "magic-string": "^0.26.1",
    "@types/node": "^17.0.21",
    "@rollup/plugin-typescript": "^8.3.1",
    "@rollup/plugin-node-resolve": "^13.1.3"
  }
}