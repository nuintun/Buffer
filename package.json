  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/Buffer/issues"
  },
  "homepage": "https://github.com/nuintun/Buffer#readme",
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.38.2",
    "typescript": "^4.1.3",
    "magic-string": "^0.25.7",
    "@types/node": "^14.14.22",
    "rollup-plugin-typescript2": "^0.29.0",
    "@rollup/plugin-node-resolve": "^11.1.1"
