{
  "name": "@nuintun/buffer",
  "version": "0.0.1",
  "description": "A buffer tool for javascript.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "test": "node tests/index.js",
    "prepublishOnly": "npm run build",
